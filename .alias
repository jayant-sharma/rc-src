################## General Purpose #########################################################
alias c "cat"
alias d "diff"
alias v "vi"
alias vd "vimdiff"
alias h 'history'
alias w "wc -l"
alias al "source ~jayants/.alias "
alias lss "ls | sort"
alias ls1 "ls -1"
alias ll "ls -lart --color=tty"
alias ld "ls -lart | grep ^d"
alias lg "ls | grep "
alias rd "rm -rf"
alias . "pwd"
alias .. "cd .."
alias ... "cd ../.."
alias .... "cd ../../.."
alias setln "v ~/.vimrc < ~/utilities/dep/setlinesed >& /dev/null"
alias unsetln "v ~/.vimrc < ~/utilities/dep/unsetlinesed >& /dev/null"
