# .cshrc

set prompt = "[%{\033[1;35m%}%m%{\033[0m%} %c] $ "
set ignoreeof
set noclobber
set notify
set filec
set autolist
set nobeep
set complete    = enhance
set autologout  = 0
set history     = 10000
set savehist    = 10000
set listmaxrows = 1000
umask 022

source ~/.alias.cshrc

clear
echo ''


